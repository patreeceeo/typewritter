{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "tsRules": {},
    "rules": {
      "object-literal-sort-keys": {
        "severity": "off"
      },
      "object-literal-shorthand": {
        "severity": "off"
      },
      "quotemark": {
        "severity": "off"
      },
      "semicolon": {
        "options": "never",
        "severity": "warning"
      },
      "no-consecutive-blank-lines": {
        "severity": "off"
      },
      "jsx-alignment": {
        "severity": "warning"
      },
      "jsx-no-lambda": true,
      "jsx-no-string-ref": true,
      "jsx-self-close": {
        "severity": "warning"
      },
      "jsx-equals-spacing": {
        "severity": "warning",
        "options": "never"
      }
    },
    "rulesDirectory": []
}
